<Sysmon schemaversion="4.30">
  <EventFiltering>
    <RuleGroup name="" groupRelation="or">
      <FileCreate onmatch="include">
        <TargetFilename condition="contains">Desktop</TargetFilename>        <!--Files on the Desktop-->
      </FileCreate>
    </RuleGroup>
  </EventFiltering>
</Sysmon>